{
  "name": "bit-silent-bot-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,vue,html,css,scss,md}'",
    "type-check": "vue-tsc -b",
    "prepare": "husky",
    "precommit": "npm run type-check && npm run format",
    "prepush": "npm-run-all --parallel type-check format"
  },
  "dependencies": {
    "ag-charts-enterprise": "10.0.1",
    "ag-charts-vue3": "10.0.1",
    "ag-grid-community": "32.0.1",
    "ag-grid-enterprise": "32.0.1",
    "ag-grid-vue3": "32.0.1",
    "axios": "1.7.2",
    "connect-history-api-fallback": "^2.0.0",
    "countries-list": "3.1.0",
    "pinia": "2.1.7",
    "view-ui-plus": "1.3.18",
    "vue": "3.4.31",
    "vue-i18n": "9.13.1",
    "vue-router": "4.4.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.0.5",
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.2",
    "prettier-plugin-vue": "1.1.6",
    "sass": "1.77.7",
    "sass-loader": "14.2.1",
    "typescript": "5.5.3",
    "vite": "5.3.3",
    "vue-tsc": "2.0.26"
  }
}
